<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             
             x:Class="PaisNaEscola.Senhas.Coordenador">
    <ContentPage.Content>
        <StackLayout>
            <Image WidthRequest="200" HeightRequest="200" Source="coordenador.png"/>
            <Entry Placeholder="Login" x:Name="LoginCoord" PlaceholderColor="LightBlue"  HeightRequest="50" Margin="50"/>
            <Entry Placeholder="Senha" x:Name="SenhaCoord" PlaceholderColor="LightBlue"  HeightRequest="50" Margin="50"/>
            <Button BackgroundColor="Red" Clicked="verificarloginsenha" Text="OK"  HeightRequest="30" Margin="200" />
            <Label />

        </StackLayout>
    </ContentPage.Content>
</ContentPage>